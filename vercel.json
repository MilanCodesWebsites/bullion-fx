{
  "rewrites": [
    {
      "source": "/dashboard/assets/(.*)",
      "destination": "/dashboard/assets/$1"
    },
    {
      "source": "/dashboard/(.*)?",
      "destination": "/dashboard/index.html"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*)?",
      "destination": "/index.html"
    }
  ]
}

